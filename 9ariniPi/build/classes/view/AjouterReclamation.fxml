<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="490.0" prefWidth="687.0" stylesheets="@gestionPaiement.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AjouterReclamationController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="498.0" prefWidth="682.0" VBox.vgrow="ALWAYS">
         <children>
            <ImageView fitHeight="660.0" fitWidth="690.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/ajouterReclamationBG.jpg" />
               </image>
            </ImageView>
            <Label layoutX="345.0" layoutY="76.0" text="Catégorie :" />
            <ComboBox fx:id="categorie" layoutX="455.0" layoutY="71.0" prefHeight="31.0" prefWidth="121.0" promptText="Choisir" />
            <Label layoutX="311.0" layoutY="180.0" text="Description :" />
            <TextArea fx:id="description" layoutX="415.0" layoutY="148.0" prefHeight="200.0" prefWidth="200.0" />
            <Button fx:id="btnAjouterReclamation" layoutX="271.0" layoutY="408.0" mnemonicParsing="false" onAction="#ajouterReclamation" text="Réclamer" />
            <Button fx:id="btnRetourReclamation" layoutX="443.0" layoutY="408.0" mnemonicParsing="false" onAction="#retourReclamation" text="Annuler" />
         </children>
    </AnchorPane>
  </children>
</VBox>
